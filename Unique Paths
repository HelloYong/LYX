public int uniquePaths(int m, int n) {
        if(m <= 0 || n<= 0)
            return 0;
        int helper[] = new int[n];
        for(int i=0;i<n;i++) {
            helper[i] = 1;
        }
        for(int i=1;i<m;i++) {
            for(int j=1;j<n;j++) {
                helper[j] = helper[j] + helper[j-1];
            }
        }
        return helper[n-1];
    }
